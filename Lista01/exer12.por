programa {
	funcao inicio() {
		/*
		F.U.A que leia dois números e calcule qual é o 
		resto da divisão do 1o pelo 2o número. 
		Exiba na tela este valor final.
		*/
		
		
		
		
		inteiro num1, num2 
		real resto
		escreva("Digte o primeiro valor\n")
		leia(num1)
		escreva("Segundo valore\n")
		leia(num2)
		resto=num1%num2  /// 
		escreva("\nO Resto é :", resto)
	}
}
