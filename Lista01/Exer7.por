	    //adição, subtração, multiplicação e divisão de A por B apresentando ao 
	    //final os quatro resultados obtidos.
	    inteiro a,b, soma, sub,div,mult
	    escreva("Digite numero 1\n")
	    leia(a)
	    escreva("Digite numero 2\n")
	    leia(b)
	    escreva("\nSoma:",a+b)
	    escreva("\nSubs:",a-b)
