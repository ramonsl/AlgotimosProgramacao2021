	    ///outra forma
	    soma=a+b
	    escreva("\nSoma:",soma)
	    sub=a-b
	    escreva("\nSub:",sub)
	    
	   div=a/b
	    escreva("\n Divisão:",div)
	   mult=a*b
	    escreva("\nMultiplicação:",mult)

		
	}
