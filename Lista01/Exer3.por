programa {
	funcao inicio() {
		//Faça um algoritmo que leia um número inteiro e imprima seu antecessor e seu sucessor.
		inteiro valor,antecessor,sucessor
		
		escreva("\nDigite o valor: ")
		leia(valor)
		antecessor= valor-1
		sucessor = valor+1
		escreva("\n sucessor: ",sucessor)
		escreva("\n antecessor: ",antecessor)
		
	}
}
