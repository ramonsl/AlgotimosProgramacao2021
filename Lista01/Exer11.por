programa {
	funcao inicio() {
		/*
		F.U.A que leia o preço de um produto e a quantidade comprada 
		e exiba para o usuário o preço que ele tem que pagar pela compra.
		*/
		real valor, total
		inteiro qtd
		escreva("Digite o valor do produto:\n")
		leia(valor)
		
		escreva("Digite a quantidade:\n")
		leia (qtd)
		
		total=valor*qtd
		
		escreva("\n O Valor total é: "+total)
		
		
		
	}
}
